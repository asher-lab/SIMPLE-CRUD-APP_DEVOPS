pipeline {
    agent any
  
    stages {
        stage("init") {
            steps {
              echo "Initialzing the APP"
            }
        }
      
      
        stage("build jar") {
            steps {
                echo "building the app"
            }
        }
      
      
        stage("build image") {
            steps {
               echo "building the image"
            }
        }
      
      
        stage("deploy") {
            steps {
               echo "deploying the image on rep"
        }
    }   
}
    
    
}
